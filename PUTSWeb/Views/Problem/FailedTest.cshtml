@model FailedTest

<div class="test-case">
    <div class="form-group ">
        @Html.LabelFor(m => m.Expected, new { @class = "control-label" })
        @Html.TextAreaFor(m => m.Expected, new { @class = "form-control", @readonly = "readonly"})
    </div>

    <div class="form-group ">
        @Html.LabelFor(m => m.Returned, new { @class = "control-label" })
        @Html.TextAreaFor(m => m.Returned, new { @class = "form-control", @readonly = "readonly"})
    </div>
</div>
